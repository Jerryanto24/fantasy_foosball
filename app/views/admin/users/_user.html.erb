<!-- index data  -->
<tr>
  <td><%= user.first_name if user.last_name %></td>
  <td><%= user.last_name if user.last_name %></td>
  <td><%= user.team_name %></td>
  <td class="text-right">
    <%= link_to edit_admin_user_path(user.id), class: "text-primary btn-just-icon" do %>
      <i class="material-icons">edit</i>
    <% end %>
  </td>
  <td class="text-right">
    <%= link_to admin_user_path(user.id), class: "text-info btn-just-icon" do %>
      <i class="material-icons">dehaze</i>
    <% end %>
  </td>
  <td class="text-right">
    <%= link_to admin_user_path(user.id), class: "text-danger btn-just-icon delete",  method: :delete, data: { confirm: 'Are you sure?', remote: true }  do %>
      <i class="material-icons">close</i>
    <% end %>
  </td>
</tr>
